[
  {% assign urls = site.pages | where: "layout", "url" %}
  {% for page in urls %}
    {
      "title": "{{ page.title }}",
      "layout": "{{ page.layout }}",
      "permalink": "{{ page.permalink }}",
      "href": "{{ page.path }}"
    } {% if forloop.rindex != 1 %},{% endif %}
  {% endfor %}
]
